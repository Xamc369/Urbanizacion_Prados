                                                    <div class="stuff_article_inner">
                                                        <span class="stuff_date">
                                                            @Html.DisplayFor(modelItem => item.Not_Fecha.Month)
                                                            <br />

                                                            <strong> @Html.DisplayFor(modelItem => item.Not_Fecha.Day)</strong>
                                                        </span>
                                                        <h2><a href="pages/single.html"> @Html.DisplayFor(modelItem => item.Not_Titulo)</a></h2>
                                                        <p>
                                                            @Html.DisplayFor(modelItem => item.Not_Descripcion)

                                                        </p>
                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>

                                                    </div>



                                                }
                                            </td>

                                        </tr>

                                    </tbody>

                                </table>








                            </div>
                        </div>




                        <div class="col-sm-5 col-md-2 col-lg-3">
