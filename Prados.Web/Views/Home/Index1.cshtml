                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td>
                                                @foreach (var item in Model.Where(n => n.Not_Estado == 'A').OrderByDescending(model => model.Id))
                                                {

                                                    <div class="card">

                                                        <div class="single_stuff wow fadeInDown">
                                                            <div class="single_stuff_img">
                                                                <a>
                                                                    <img src="@Url.Content(item.ImageUrl)" alt="" class="f-left imagenNoticia" />

                                                                </a>
                                                            </div>
                                                            <div class="">
