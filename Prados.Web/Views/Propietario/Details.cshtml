@model Prados.Web.Data.Entities.Propietariostbl


@{
    ViewData["Title"] = "Details";
}

<link rel="stylesheet" href="~/css/tabla/paginate.css">
<link rel="stylesheet" href="~/css/main.css">
<link rel="stylesheet" href="~/css/adminlte.min.css">
<script type="text/javascript" src="~/js/tabla/paginate.js"></script>

<style>
    * {
        font-family: sans-serif;
    }
</style>
<br />



<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h2 class="text-titles" style="text-align:center">
                        <i>
                            <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="currentColor" class="bi bi-person-plus-fill" viewBox="0 0 16 16">
                                <path d="M1 14s-1 0-1-1 1-4 6-4 6 3 6 4-1 1-1 1H1zm5-6a3 3 0 1 0 0-6 3 3 0 0 0 0 6z" />
                                <path fill-rule="evenodd" d="M13.5 5a.5.5 0 0 1 .5.5V7h1.5a.5.5 0 0 1 0 1H14v1.5a.5.5 0 0 1-1 0V8h-1.5a.5.5 0 0 1 0-1H13V5.5a.5.5 0 0 1 .5-.5z" />
                            </svg>
                        </i><strong> Información</strong>
                    </h2>

                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a asp-area="" asp-controller="Home" asp-action="Index1">Inicio</a></li>
                        <li class="breadcrumb-item active">Formulario General</li>
                    </ol>
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>


    <!-- Main content -->
    <section class="content">
        <div class="container-fluid">
            <div class="row">

                <div class="form-group col-md-6">
                    <div class="card">
                        <div class="card-header">
                            <h3 style=""> <strong>Usuario</strong></h3>
                        </div>
                        <div class="card-body">
                            <blockquote class="blockquote mb-0">
                                <dl>
                                    <dd>
                                        <strong> @Html.DisplayNameFor(model => model.User.Pro_Nombres): </strong>

                                        @Html.DisplayFor(model => model.User.Pro_Nombres)
                                    </dd>
                                    <dd>
                                        <strong> @Html.DisplayNameFor(model => model.User.Pro_Apellidos): </strong>
                                        @Html.DisplayFor(model => model.User.Pro_Apellidos)
                                    </dd>
                                    <dd>
                                        <strong>  @Html.DisplayNameFor(model => model.User.Pro_Lote): </strong>
                                        @Html.DisplayFor(model => model.User.Pro_Lote)
                                    </dd>
                                    <dd>
                                        <strong>  @Html.DisplayNameFor(model => model.User.Pro_Identificacion): </strong>
                                        @Html.DisplayFor(model => model.User.Pro_Identificacion)
                                    </dd>
                                    <dd>
                                        <strong>  @Html.DisplayNameFor(model => model.User.Pro_Identificacion): </strong>
                                        @Html.DisplayFor(model => model.User.Email)
                                    </dd>
                                </dl>

                            </blockquote>
                        </div>
                    </div>
                </div>
                <div class="form-group col-md-6">
                    <div class="card">
                        <div class="card-header">
                            <h3><strong>Acción</strong></h3>
                        </div>
                        <div class="card-body">
                            <blockquote class="blockquote mb-0">
                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="" style="text-align:center;text-decoration-color:white">
                                            <a asp-action="AddVehiculo" asp-route-id="@Model.Id" class="btn btn-primary"><i><img src="~/images/icon/car_vehicle_transport_icon_144665.png" width="50" height="50"></i></a>
                                            <br />
                                            <a> Registrar Vehiculo</a>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">

                                        <div class="" style="text-align:center;text-decoration-color:white">
                                            <a asp-action="AddPago" asp-route-id="@Model.Id" class="btn btn-success">
                                                <i>
                                                    <svg xmlns="http://www.w3.org/2000/svg" width="50px" height="50px" fill="currentColor" class="bi bi-cash-stack" viewBox="0 0 16 16">
                                                        <path d="M1 3a1 1 0 0 1 1-1h12a1 1 0 0 1 1 1H1zm7 8a2 2 0 1 0 0-4 2 2 0 0 0 0 4z" />
                                                        <path d="M0 5a1 1 0 0 1 1-1h14a1 1 0 0 1 1 1v8a1 1 0 0 1-1 1H1a1 1 0 0 1-1-1V5zm3 0a2 2 0 0 1-2 2v4a2 2 0 0 1 2 2h10a2 2 0 0 1 2-2V7a2 2 0 0 1-2-2H3z" />
                                                    </svg>
                                                </i>
                                            </a>
                                            <br />
                                            <a> Registrar Pago</a>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-6">

                                        <div class="" style="text-align:center;text-decoration-color:white">
                                            <a asp-action="AddNegocio" asp-route-id="@Model.Id" class="btn btn-warning">
                                                <i>
                                                    <svg xmlns="http://www.w3.org/2000/svg" width="50px" height="50px" fill="currentColor" class="bi bi-shop" viewBox="0 0 16 16">
                                                        <path d="M2.97 1.35A1 1 0 0 1 3.73 1h8.54a1 1 0 0 1 .76.35l2.609 3.044A1.5 1.5 0 0 1 16 5.37v.255a2.375 2.375 0 0 1-4.25 1.458A2.371 2.371 0 0 1 9.875 8 2.37 2.37 0 0 1 8 7.083 2.37 2.37 0 0 1 6.125 8a2.37 2.37 0 0 1-1.875-.917A2.375 2.375 0 0 1 0 5.625V5.37a1.5 1.5 0 0 1 .361-.976l2.61-3.045zm1.78 4.275a1.375 1.375 0 0 0 2.75 0 .5.5 0 0 1 1 0 1.375 1.375 0 0 0 2.75 0 .5.5 0 0 1 1 0 1.375 1.375 0 1 0 2.75 0V5.37a.5.5 0 0 0-.12-.325L12.27 2H3.73L1.12 5.045A.5.5 0 0 0 1 5.37v.255a1.375 1.375 0 0 0 2.75 0 .5.5 0 0 1 1 0zM1.5 8.5A.5.5 0 0 1 2 9v6h1v-5a1 1 0 0 1 1-1h3a1 1 0 0 1 1 1v5h6V9a.5.5 0 0 1 1 0v6h.5a.5.5 0 0 1 0 1H.5a.5.5 0 0 1 0-1H1V9a.5.5 0 0 1 .5-.5zM4 15h3v-5H4v5zm5-5a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1v3a1 1 0 0 1-1 1h-2a1 1 0 0 1-1-1v-3zm3 0h-2v3h2v-3z" />
                                                    </svg>
                                                </i>
                                            </a>
                                            <br />
                                            <a> Registrar Negocio</a>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="" style="text-align:center;text-decoration-color:white">
                                            <a asp-action="Index" class="btn btn-info">
                                                <i>
                                                    <svg xmlns="http://www.w3.org/2000/svg" width="50px" height="50px" fill="currentColor" class="bi bi-arrow-return-left" viewBox="0 0 16 16">
                                                        <path fill-rule="evenodd" d="M14.5 1.5a.5.5 0 0 1 .5.5v4.8a2.5 2.5 0 0 1-2.5 2.5H2.707l3.347 3.346a.5.5 0 0 1-.708.708l-4.2-4.2a.5.5 0 0 1 0-.708l4-4a.5.5 0 1 1 .708.708L2.707 8.3H12.5A1.5 1.5 0 0 0 14 6.8V2a.5.5 0 0 1 .5-.5z" />
                                                    </svg>
                                                </i>
                                            </a>
                                            <br />
                                            <a> Regresar</a>
                                        </div>
                                    </div>
                                </div>
                            </blockquote>
                        </div>
                    </div>
                </div>
                @if (Model.Vehiculos.Count == 0)
                {
                    <div class="form-group col-md-12">
                        <div class="card">
                            <div class="card-header">
                                <h3><strong>No registra vehiculos</strong></h3>
                            </div>
                            <div class="card-body">
                            </div>
                        </div>
                    </div>
                }
                else
                {
                    <!-- Tabla vehiculos-->

                    <div class="form-group col-md-12">
                        <div class="card">
                            <div class="card-header">
                                <h3><strong>Vehiculos</strong></h3>
                            </div>
                            <div class="card-body">
                                <div id="example3_wrapper" class="dataTables_wrapper dt-bootstrap4">
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <table id="example5" class="table table-responsive " role="grid" aria-describedby="example1_info">

                                                <thead class="thead-light">
                                                    <tr>
                                                        <th>
                                                            @Html.DisplayNameFor(model => model.Vehiculos.FirstOrDefault().Veh_Codigo)
                                                        </th>
                                                        <th>
                                                            @Html.DisplayNameFor(model => model.Vehiculos.FirstOrDefault().Veh_Placa)
                                                        </th>
                                                        <th>
                                                            @Html.DisplayNameFor(model => model.Vehiculos.FirstOrDefault().ImageUrl)
                                                        </th>
                                                        <th>Editar</th>
                                                        <th>Eliminar</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    @foreach (var item in Model.Vehiculos.Where(v => v.Veh_Estado == 'A'))
                                                    {
                                                        <tr>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.Veh_Codigo)
                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.Veh_Placa)
                                                            </td>
                                                            <td>
                                                                @if (!string.IsNullOrEmpty(item.ImageUrl))
                                                                {
                                                                    <img src="@Url.Content(item.ImageUrl)" alt="Image" style="width:200px;height:200px;max-width: 100%; height: auto;" />
                                                                }
                                                            </td>
                                                            <td>
                                                                <a asp-action="EditVehiculo" asp-route-id="@item.Id" class="btn btn-warning"><img src="~/images/icon/editar.png" width="30" height="30"></a> 
                                                                @*<a asp-action="DetailsPago" asp-route-id="@item.Id" class="btn btn-success"><i class="glyphicon glyphicon-list-alt"></i></a> ]*@
                                                            </td>
                                                            <td>
                                                                <a asp-action="DeleteVehiculo" asp-route-id="@item.Id" class="btn btn-danger center-block"><img src="~/images/icon/eliminar.png" width="30" height="30"></a>
                                                            </td>
                                                        </tr>
                                                    }
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                }
                @if (Model.Negocio.Count == 0)
                {

                    <div class="form-group col-md-12">
                        <div class="card">
                            <div class="card-header">
                                <h3><strong>No registra negocios</strong></h3>
                            </div>
                            <div class="card-body">
                            </div>
                        </div>
                    </div>
                }
                else
                {
                    <div class="form-group col-md-12">
                        <div class="card">
                            <div class="card-header">
                                <h3><strong>Negocios</strong></h3>
                            </div>
                            <div class="card-body">
                                <div id="example2_wrapper" class="dataTables_wrapper dt-bootstrap4">
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <table id="example3" class="table table-responsive " role="grid" aria-describedby="example1_info">

                                                <thead class="thead-light">
                                                    <tr>
                                                        <th>
                                                            @Html.DisplayNameFor(model => model.Negocio.FirstOrDefault().Neg_Nombre)
                                                        </th>
                                                        <th>
                                                            @Html.DisplayNameFor(model => model.Negocio.FirstOrDefault().ImageUrl)
                                                        </th>
                                                        <th>Acción</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    @foreach (var item in Model.Negocio.Where(n => n.Neg_Estado == 'A'))
                                                    {
                                                        <tr>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.Neg_Nombre)
                                                            </td>
                                                            <td>
                                                                @if (!string.IsNullOrEmpty(item.ImageUrl))
                                                                {
                                                                    <img src="@Url.Content(item.ImageUrl)" alt="Image" style="width:200px;height:200px;max-width: 100%; height: auto;" />
                                                                }
                                                            </td>
                                                            <td>
                                                                <a asp-action="EditNegocio" asp-route-id="@item.Id" class="btn btn-warning"><img src="~/images/icon/editar.png" width="30" height="30"></a> ]
                                                                <a asp-action="DetailsNegocio" asp-route-id="@item.Id" class="btn btn-success"><img src="~/images/icon/detalle.png" width="30" height="30"></a> ]
                                                                <a asp-action="DeleteNegocio" asp-route-id="@item.Id" class="btn btn-danger center-block"><img src="~/images/icon/eliminar.png" width="30" height="30"></a>
                                                            </td>
                                                        </tr>
                                                    }
                                                </tbody>

                                            </table>
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                }
                @if (Model.Pagos.Count == 0)
                {
                    <div class="form-group col-md-12">
                        <div class="card">
                            <div class="card-header">
                                <h3><strong>No tiene Pagos todavia</strong></h3>
                            </div>
                            <div class="card-body">


                            </div>
                        </div>
                    </div>
                }
                else
                {
                    <div class="form-group col-md-12">
                        <div class="card">
                            <div class="card-header">
                                <h3><strong>Pagos</strong></h3>
                            </div>
                            <div class="card-body">
                                <div id="example1_wrapper" class="dataTables_wrapper dt-bootstrap4">
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <table id="example1" class="table table-responsive " role="grid" aria-describedby="example1_info">
                                                    <thead class="thead-light">
                                                        <tr>
                                                            <th>
                                                                @Html.DisplayNameFor(model => model.Pagos.FirstOrDefault().PAG_FECHAPAGADO)
                                                            </th>
                                                            <th>
                                                                @Html.DisplayNameFor(model => model.Pagos.FirstOrDefault().Anio)
                                                            </th>
                                                            <th>
                                                                @Html.DisplayNameFor(model => model.Pagos.FirstOrDefault().Mes)
                                                            </th>
                                                            <th>
                                                                @Html.DisplayNameFor(model => model.Pagos.FirstOrDefault().Val)
                                                            </th>
                                                            <th>
                                                                @Html.DisplayNameFor(model => model.Pagos.FirstOrDefault().Tipos)
                                                            </th>
                                                            <th>
                                                                @Html.DisplayNameFor(model => model.Pagos.FirstOrDefault().PuntodePago)
                                                            </th>
                                                            <th>Acción</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        @foreach (var item in Model.Pagos.Where(pa => pa.PAG_ESTADO == 'A'))
                                                        {
                                                            <tr>
                                                                <td>
                                                                    @Html.DisplayFor(modelItem => item.PAG_FECHAPAGADO)
                                                                </td>
                                                                <td>
                                                                    @Html.DisplayFor(modelItem => item.Anio.Ani_Descripcion)
                                                                </td>
                                                                <td>
                                                                    @Html.DisplayFor(modelItem => item.Mes.Mes_Descripcion)
                                                                <td>
                                                                    @Html.DisplayFor(modelItem => item.Val.Val_Valor)
                                                                </td>
                                                                <td>
                                                                    @Html.DisplayFor(modelItem => item.Tipos.Tip_Descripcion)
                                                                </td>
                                                                <td>
                                                                    @Html.DisplayFor(modelItem => item.PuntodePago.Pun_Descripcion)
                                                                </td>
                                                                <td>
                                                                    @*<a asp-action="EditPago" asp-route-id="@item.Id" class="btn btn-warning"><i class="glyphicon glyphicon-edit"></i></a> ]*@
                                                                    @*<a asp-action="DetailsPago" asp-route-id="@item.Id" class="btn btn-success"><i class="glyphicon glyphicon-list-alt"></i></a> ]*@
                                                                    
                                                                    <a asp-action="DeletePagos" asp-route-id="@item.Id" class="btn btn-danger center-block"><img src="~/images/icon/eliminar.png" width="30" height="30"></a>
                                                                </td>
                                                            </tr>
                                                        }
                                                    </tbody>
                                                
                                            </table>
                                        </div>
                                    </div>
                               
                                </div>
                            </div>
                        </div>
                    </div>
                }
                <partial name="_DeleteDialog" />
            </div>
        </div>
    </section>
</div>



@section scripts{


    <script type="text/javascript">
        $(document).ready(function () {
            $("#example1").DataTable({
                "responsive": true, "lengthChange": false, "autoWidth": false,
                "buttons": ["copy", "csv", "excel", "pdf", "print", "colvis"]
            }).buttons().container().appendTo('#example1_wrapper .col-md-6:eq(0)');
            $('#example2').DataTable({
                "paging": true,
                "lengthChange": false,
                "searching": false,
                "ordering": true,
                "info": true,
                "autoWidth": false,
                "responsive": true,
            });


        });
    </script>


    <script type="text/javascript">
        $(document).ready(function () {
            $("#example3").DataTable({
                "responsive": true, "lengthChange": false, "autoWidth": false,
                "buttons": ["copy", "csv", "excel", "pdf", "print", "colvis"]
            }).buttons().container().appendTo('#example2_wrapper .col-md-6:eq(0)');
            $('#example4').DataTable({
                "paging": true,
                "lengthChange": false,
                "searching": false,
                "ordering": true,
                "info": true,
                "autoWidth": false,
                "responsive": true,
            });


        });
    </script>


    <script type="text/javascript">
        $(document).ready(function () {
            $("#example5").DataTable({
                "responsive": true, "lengthChange": false, "autoWidth": false,
                "buttons": ["copy", "csv", "excel", "pdf", "print", "colvis"]
            }).buttons().container().appendTo('#example3_wrapper .col-md-6:eq(0)');
            $('#example6').DataTable({
                "paging": true,
                "lengthChange": false,
                "searching": false,
                "ordering": true,
                "info": true,
                "autoWidth": false,
                "responsive": true,
            });


        });
    </script>


}

