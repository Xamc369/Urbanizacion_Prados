@model IEnumerable<Prados.Web.Data.Entities.Propietariostbl>

@{
    ViewData["Title"] = "Lista de Propietarios";
}









    <div class="content-wrapper">

        <section class="content">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <div class="card-header">


                                <h2 style="text-align:center">Lista Propietarios</h2>


                                <p>
                                    <a asp-action="Create" class="btn btn-success"><img src="~/images/icon/ADD.png" width="30" height="30"></a>
                                </p>
                                
                            </div>

                           
                            <div class="card-body ">
                                <div id="example1_wrapper" class="dataTables_wrapper dt-bootstrap4">
                                    <div class="row">
                                        <div class="form-group col-md-12">
                                            <table id="example1" class="table table-responsive  " role="grid" aria-describedby="example1_info">
                                                <thead class="thead-light">
                                                    <tr role="row">
                                                        <th class="sorting_asc" tabindex="0" aria-controls="example1" rowspan="1" colspan="1" aria-sort="ascending" aria-label="Rendering engine: activate to sort column descending">
                                                            @Html.DisplayNameFor(model => model.User.Pro_Lote)
                                                        </th>
                                                        <th style="text-align:center">
                                                            @Html.DisplayNameFor(model => model.User.Pro_Nombres)
                                                        </th>
                                                        <th style="text-align:center">
                                                            @Html.DisplayNameFor(model => model.User.Pro_Apellidos)
                                                        </th>
                                                        <th style="text-align:center">
                                                            @Html.DisplayNameFor(model => model.User.Pro_Identificacion)
                                                        </th>
                                                        <th style="text-align:center">
                                                            @Html.DisplayNameFor(model => model.User.Pro_Telefono)
                                                        </th>
                                                        <th style="text-align:center"> Editar</th>
                                                        <th style="text-align:center"> Detalles</th>
                                                        <th style="text-align:center"> Eliminar</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    @foreach (var item in Model)
                                                    {
                                                        <tr class="odd">
                                                            <td style="text-align:center">
                                                                @Html.DisplayFor(modelItem => item.User.Pro_Lote)
                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.User.Pro_Nombres)
                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.User.Pro_Apellidos)
                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.User.Pro_Identificacion)
                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.User.Pro_Telefono)
                                                            </td>

                                                            <td>
                                                                <a asp-action="Edit" asp-route-id="@item.Id" class="btn btn-warning center-block"><img src="~/images/icon/editar.png" width="30" height="30"></a>

                                                            </td>
                                                            <td>
                                                                <a asp-action="Details" asp-route-id="@item.Id" class="btn btn-success center-block"><img src="~/images/icon/detalle.png" width="30" height="30"></a>

                                                            </td>
                                                            <td>
                                                                <a asp-action="Delete" asp-route-id="@item.Id" class="btn btn-danger center-block"><img src="~/images/icon/eliminar.png" width="30" height="30"></a>
                                                                @*<button data-id="@item.Id" class="btn btn-danger deleteItem center-block" data-toggle="modal" data-target="#deleteDialogo"><i class="glyphicon glyphicon-trash"></i></button>*@
                                                            </td>
                                                        </tr>
                                                    }
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                    @*<div class="row">
                    <div class="col-sm-12 col-md-5">
                        <div class="dataTables_info" id="example1_info" role="status" aria-live="polite">
                            Mostrando 1 de 5 de 5 entradas
                        </div>
                    </div>
                    <div class="col-sm-12 col-md-7si amo">
                        <div class="dataTables_paginate paging_simple_numbers" id="example1_paginate">
                            <ul class="pagination">
                                <li class="paginate_button page-item previous disabled" id="example1_previous">
                                    <a href="#" aria-controls="example1" data-dt-idx="0" tabindex="0" class="page-link">Anterior</a>
                                </li>
                                <li class="paginate_button page-item active">
                                    <a href="#" aria-controls="example1" data-dt-idx="1" tabindex="0" class="page-link">1</a>
                                </li>
                                <li class="paginate_button page-item next disabled" id="example1_next">
                                    <a href="#" aria-controls="example1" data-dt-idx="2" tabindex="0" class="page-link">Siguiente</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>*@
                                </div>
                            </div>





                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
    <!-- Control Sidebar -->
    <aside class="control-sidebar control-sidebar-dark">
        <!-- Control sidebar content goes here -->
    </aside>
    <!-- /.control-sidebar -->
    <!-- jQuery -->
   
 

    @section scripts{


        <script type="text/javascript">
            $(document).ready(function () {
                $("#example1").DataTable({
                    "responsive": true, "lengthChange": false, "autoWidth": false,
                    "buttons": ["copy", "csv", "excel", "pdf", "print"]
                }).buttons().container().appendTo('#example1_wrapper .col-md-6:eq(0)');
                $('#example2').DataTable({
                    "paging": true,
                    "lengthChange": false,
                    "searching": false,
                    "ordering": true,
                    "info": true,
                    "autoWidth": false,
                    "responsive": true,
                });


            });
        </script>

    }


